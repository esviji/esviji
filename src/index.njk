<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="initial-scale=1, maximum-scale=1, user-scalable=no"
    />
    <title>{{ name }}</title>
    <meta name="description" content="{{ description }}" />

    <meta property="og:type" content="game" />
    <meta property="og:title" content="{{ name }}" />
    <meta property="og:description" content="{{ description }}" />
    <meta property="og:url" content="{{ url }}" />
    <meta
      property="og:image"
      content="{{ url }}img/esviji-screenshot.png"
    />

    <meta name="twitter:card" content="summary" />
    <meta name="twitter:creator" content="@nhoizey" />
    <meta name="twitter:site" content="@esviji" />
    <meta name="twitter:title" content="{{ name }}" />
    <meta name="twitter:description" content="{{ description }}" />
    <meta
      name="twitter:image"
      content="{{ url }}img/esviji-mockup.jpg"
    />

    <!-- build:css css/styles.css -->
    <link
      rel="stylesheet"
      media="all"
      href="sass/styles.scss"
      type="text/css"
    />
    <!-- endbuild -->

    <!-- iOS features -->
    <meta name="application-name" content="{{ name }}" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="format-detection" content="telephone=no" />
    <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="/img/esviji-logo-180.png"
    />

    <!-- Android features -->
    <meta name="theme-color" content="#444444" />

    <!-- favicon -->
    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="/img/esviji-logo-32.png"
    />

    <!-- Web manifest -->
    <link
      rel="manifest"
      href="manifest.webmanifest"
      crossorigin="use-credentials"
    />

    <style>
      [aria-hidden='true'] {
        display: none;
      }
    </style>

    <script src="/js/vendor/modernizr-custom.js"></script>

    <!-- Google Analytics -->
    <script>
      window.ga = function() {
        ga.q.push(arguments);
      };
      ga.q = [];
      ga.l = +new Date();
      ga('create', 'UA-1655999-4', 'auto');
      ga('set', 'transport', 'beacon');
      ga('set', 'anonymizeIp', true);
      // ga('set', 'dimension4', 'online');
      ga('send', 'pageview');
    </script>
    <script
      src="https://www.google-analytics.com/analytics.js"
      async
      defer
    ></script>
  </head>
  <body>
    <script type="application/ld+json">
    {
      "@context": "http://schema.org/",
      "@type": "VideoGame",

      "name": "{{ name }}",
      "softwareVersion": "{{ version }}",
      "description": "{{ description }}",

      "url": "{{ url }}",
      "gameLocation": "{{ url }}",

      "applicationCategory": "GameApplication",
      "applicationSubCategory": "WebApplication",
      "genre": ["Puzzle game", "Logic game"],
      "playMode": "SinglePlayer",
      "isFamilyFriendly": true,
      "sameAs": "https://en.wikipedia.org/wiki/Puzzle_Bobble",

      "image": "{{ url }}img/esviji-logo.png",
      "screenshot": "{{ url }}img/esviji-screenshot.png",
      "trailer": {
        "@type": "VideoObject",
        "url": "https://www.youtube.com/watch?v=R2rxU9cVSqw",
