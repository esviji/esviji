    <header id="description">
      <h1>{{ name }}</h1>
      <p>{{ description }}</p>
      <p class="icon">
        <img src="img/esviji-logo-180.png" alt="esviji logo" />
      </p>
      <p>Version {{ version }}</p>
    </header>
    <section id="home" aria-hidden="true">
      <header>
        <h1>{{ name }}</h1>
        {% include './svg/logo.svg' %}
      </header>
      <ul class="controls">
        <li>
          <a href="/play" class="play">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconPlay" />
            </svg>
            <span>play</span>
          </a>
        </li>
        <li>
          <a href="/scores" class="scores">
            {% include './svg/scores.svg' %}
            <span>scores</span>
          </a>
        </li>
        <li>
          <a href="/sound" class="sound">
            <svg class="on" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconSoundOn" />
            </svg>
            <svg class="off" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconSoundOff" />
            </svg>
            <span>sound</span>
          </a>
        </li>
        <li>
          <a href="/about" class="about">
            {% include './svg/about.svg' %}
            <span>about</span>
          </a>
        </li>
      </ul>
    </section>
    <section id="play" aria-hidden="true">
      <h1>{{ name }}</h1>
      <div class="progress">
        <div><span class="score">0</span></div>
        <ul>
          <li>
            {% include './svg/heart.svg' %}
            <span class="lives"></span>
          </li>
          <li>
            {% include './svg/ladder.svg' %}
            <span class="level"></span>
          </li>
        </ul>
      </div>
      <ul class="controls">
        <li>
          <a href="/pause" class="pause">
            {% include './svg/pause.svg' %}
          </a>
        </li>
      </ul>
      {% include './svg/board.svg' %}
    </section>
    <section id="pause" aria-hidden="true">
      <h2>Game paused</h2>
      <ul class="controls">
        <li>
          <a href="/play" class="resume">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconPlay" />
            </svg>
            <span>resume</span>
          </a>
        </li>
        <li>
          <a href="/play" class="restart">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconRestart" />
            </svg>
            <span>restart</span>
          </a>
        </li>
        <li>
          <a href="/sound" class="sound">
            <svg class="on" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconSoundOn" />
            </svg>
            <svg class="off" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconSoundOff" />
            </svg>
            <span>sound</span>
          </a>
        </li>
        <li>
          <a href="/" class="exit">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconExit" />
            </svg>
            <span>exit</span>
          </a>
        </li>
      </ul>
    </section>
    <section id="gameover" aria-hidden="true" class="overlay">
      <h2>Game over</h2>
      <p class="message"></p>
      <p class="score"></p>
      <ul class="controls">
        <li>
          <a href="/play" class="restart">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconRestart" />
            </svg>
            <span>restart</span>
          </a>
        </li>
        <li>
          <a href="/" class="exit">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconExit" />
            </svg>
            <span>exit</span>
          </a>
        </li>
      </ul>
    </section>
    <section id="scores" aria-hidden="true">
      <h2>Best scores</h2>
      <ol class="highscores"></ol>
      <ul class="controls">
        <li>
          <a href="/" class="exit">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconExit" />
            </svg>
            <span>exit</span>
          </a>
        </li>
      </ul>
    </section>
    <section id="about" aria-hidden="true">
      <h2>About esviji</h2>
      <div class="about">
        <div class="description">
          <p>This is esviji version <span id="version">{{ version }}</span> for <span id="platform">{{ platform }}</span>.</p>
          <p>
            &copy; 1992-2020
            <a href="https://nicolas-hoizey.com/">Nicolas Hoizey</a>
          </p>
          <p>{{ description }}</p>
          <p>
            If you have any issue with the game, or an idea to make it even
            better, please
            <a href="https://github.com/nhoizey/esviji/issues/new"
              >open an issue</a
            >
            with the details.
          </p>
        </div>
        <p class="social">
          <a href="https://twitter.com/esviji">{% include './svg/twitter.svg' %}</a>
          <a href="https://facebook.com/esviji">{% include './svg/facebook.svg' %}</a>
        </p>
      </div>
      <ul class="controls">
        <li>
          <a href="/" class="exit">
            <svg class="icon" xmlns:xlink="http://www.w3.org/1999/xlink">
              <use xlink:href="#iconExit" />
            </svg>
            <span>exit</span>
          </a>
        </li>
      </ul>
    </section>
    <script src="js/vendor/firebase-2.3.2.js"></script>
    <script src="js/esviji.js"></script>
    <script>
      let sw = '/service-worker.js';
      if ('serviceWorker' in navigator) {
        window.addEventListener('load', () => {
          navigator.serviceWorker.register(sw);
        });
      }
    </script>
  </body>
</html>
