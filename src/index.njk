          "releasedEvent": {
            "@type": "PublicationEvent",
            "isAccessibleForFree": true,
            "startDate": "2013-01-23",
            "url": "{{ url }}"
          },
          "offers": {
            "@type": "Offer",
            "availability": "http://schema.org/InStock",
            "price": "0.00",
            "priceCurrency": "EUR",
            "url": "{{ url }}"
          }
        }
