{
  "name": "esviji",
  "version": "2.4.3",
  "title": "esviji, a free puzzle game for the Web",
  "description": "Play esviji on any device with your browser! You'll have to destroy balls based on their colors and place on the board.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/esviji/esviji.git"
  },
  "keywords": [
    "game",
    "svg",
    "responsive"
  ],
  "author": "Nicolas Hoizey",
  "license": "MIT",
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "rimraf dist",
    "sw": "rollup -c && workbox injectManifest ./workbox.config.js",
    "start": "npm run clean && parcel src/index.html",
    "build": "npm run clean && parcel build src/index.html && npm run sw"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "howler": "^2.1.3",
    "jquery": "^3.5.0",
    "jsonfile": "^2.3.1",
    "minimatch": "^3.0.0",
    "mousetrap": "^1.6.5",
    "natives": "^1.1.6",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-entrypoint-hashmanifest": "^0.1.2",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-terser": "^5.3.0",
    "store": "^2.0.12",
    "workbox-broadcast-update": "5.1.2",
    "workbox-cacheable-response": "5.1.2",
    "workbox-cli": "^5.1.2",
    "workbox-core": "5.1.2",
    "workbox-expiration": "5.1.2",
    "workbox-google-analytics": "5.1.2",
    "workbox-precaching": "5.1.2",
    "workbox-routing": "5.1.2",
    "workbox-strategies": "5.1.2"
  },
  "dependencies": {}
}
