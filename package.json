{
  "name": "esviji",
  "version": "2.5.0",
  "description": "esviji is a free puzzle game for the Web. Destroy balls based on their colors and place on the board.",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/esviji/esviji.git"
  },
  "keywords": [
    "game",
    "svg",
    "responsive"
  ],
  "author": "Nicolas Hoizey",
  "license": "MIT",
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "rimraf dist/*",
    "sw": "rollup -c && workbox injectManifest ./workbox.config.js",
    "start": "npm run clean && parcel src/index.njk",
    "build": "npm run clean && parcel build src/index.njk --public-url https://play.esviji.com && npm run sw"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "howler": "^2.1.3",
    "jquery": "^3.5.0",
    "mousetrap": "^1.6.5",
    "nunjucks": "^3.2.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-nunjucks": "^2.2.0",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "sass": "^1.26.5",
    "store": "^2.0.12",
    "workbox-broadcast-update": "5.1.3",
    "workbox-cacheable-response": "5.1.3",
    "workbox-cli": "^5.1.3",
    "workbox-core": "5.1.3",
    "workbox-expiration": "5.1.3",
    "workbox-google-analytics": "5.1.3",
    "workbox-precaching": "5.1.3",
    "workbox-routing": "5.1.3",
    "workbox-strategies": "5.1.3"
  }
}
